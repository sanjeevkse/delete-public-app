{
  "name": "feed-backend",
  "version": "1.0.0",
  "description": "Production-grade Express + Sequelize backend with TypeScript and Lens.js integration.",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "prestart": "npm run build",
    "start": "nodemon dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier \"**/*.{js,ts,tsx,json,md}\" --write",
    "format:check": "prettier \"**/*.{js,ts,tsx,json,md}\" --check",
    "migratessss": "sequelize db:migrate",
    "migratessss:undo": "sequelize db:migrate:undo",
    "migratessss:redo": "sequelize db:migrate:undo && sequelize db:migrate",
    "seedssss": "sequelize db:seed:all",
    "seedssss:undo": "sequelize db:seed:undo:all"
  },
  "keywords": [
    "express",
    "sequelize",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@lensjs/express": "^1.4.0",
    "@lensjs/watchers": "^1.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "firebase-admin": "13.6.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.11.3",
    "pino": "^8.17.2",
    "sequelize": "^6.37.3",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.11.30",
    "@types/uuid": "11.0.0",
    "@types/validator": "^13.11.8",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.0.11",
    "nodemon": "3.1.10",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.3"
  }
}
