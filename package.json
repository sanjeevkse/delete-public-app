{
  "name": "feed-backend",
  "version": "1.0.0",
  "description": "Production-grade Express + Sequelize backend with TypeScript and Lens.js integration.",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "lint": "eslint . --ext .ts",
    "migratessss": "sequelize db:migrate",
    "migratessss:undo": "sequelize db:migrate:undo",
    "migratessss:redo": "sequelize db:migrate:undo && sequelize db:migrate",
    "seedssss": "sequelize db:seed:all",
    "seedssss:undo": "sequelize db:seed:undo:all"
  },
  "keywords": [
    "express",
    "sequelize",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "jsonwebtoken": "^9.0.2",
    "lens.js": "^1.0.4",
    "mysql2": "^3.11.3",
    "pino": "^8.17.2",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.30",
    "@types/validator": "^13.11.8",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "ts-node": "^10.9.2",
    "pino-pretty": "^11.0.0",
    "ts-node-dev": "^2.0.0",
    "sequelize-cli": "^6.6.2",
    "typescript": "^5.4.3"
  }
}
